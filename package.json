{
  "name": "@corets/form",
  "description": "Easy to use form library that works everywhere, including React",
  "version": "0.0.1-21",
  "author": "Maxim Kott",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "keywords": [
    "corets",
    "form"
  ],
  "files": [
    "/dist"
  ],
  "repository": "github:corets/form",
  "homepage": "https://corets.github.io/form",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "test": "jest",
    "coverage": "jest --coverage && echo '\nOpen \"coverage/lcov-report/index.html\" in your browser\n'",
    "lint": "tsc --noEmit",
    "format": "prettier --write .",
    "build": "rm -rf dist && rollup -c",
    "release": "np --no-release-draft --tag next",
    "preview": "np --no-release-draft --tag dev",
    "prepublish": "yarn run build"
  },
  "dependencies": {
    "@corets/accessor": "^0.1.0-0",
    "@corets/schema": "^0.1.0-0",
    "@corets/store": "^0.1.0-0",
    "@corets/value": "^0.1.0-0",
    "fast-deep-equal": "^3.1.3",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@corets/promise-helpers": "^0.1.0-0",
    "@types/jest": "^26.0.24",
    "@types/lodash": "^4.14.171",
    "esbuild": "^0.12.15",
    "jest": "^27.0.6",
    "np": "^7.2.0",
    "prettier": "2.2.1",
    "rollup": "^2.52.8",
    "rollup-plugin-dts": "^3.0.2",
    "rollup-plugin-esbuild": "^4.5.0",
    "ts-jest": "^27.0.3",
    "typescript": "^4.3.5"
  }
}
